F_UPDATE_CLONE_PROCESSING_STATUS_INITIALIZE_TASK()
{
   F_ECHO "updating the CLONE_PROCESSING_STATUS table ..."
   F_ECHO "update system.CLONE_PROCESSING_STATUS set CURRENT_STATUS='INITIATED' , FINAL_STATUS=NULL where REFRESH_TASK_NAME='$1';"
   DB_CONN_SYSTEM << EOF >> ${LOGFILE} 2>&1
   update system.CLONE_PROCESSING_STATUS set CURRENT_STATUS='INITIATED' , FINAL_STATUS=NULL where REFRESH_TASK_NAME='$1';
   commit;
EOF
}